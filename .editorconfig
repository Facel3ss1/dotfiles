# EditorConfig is awesome: https://EditorConfig.org

root = true

[*]
indent_style = space
indent_size = 4
charset = utf-8
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true

[*.yml]
indent_size = 2

[lazy-lock.json]
indent_size = 2
insert_final_newline = false

[private_dot_config/git/**]
indent_style = tab
